{
    "name": "salines/cakephp-mail-interceptor",
    "description": "CakePHP plugin to intercept and redirect outgoing emails to a specified address. Useful for development and staging environments.",
    "type": "cakephp-plugin",
    "license": "MIT",
    "keywords": [
        "cakephp",
        "plugin",
        "mail",
        "email",
        "interceptor",
        "development",
        "testing"
    ],
    "require": {
        "php": ">=8.1",
        "cakephp/cakephp": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0 || ^11.0"
    },
    "autoload": {
        "psr-4": {
            "MailInterceptor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MailInterceptor\\Test\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
